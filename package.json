    "clsx": "^2.1.1",
    "framer-motion": "^12.23.6",
    "gsap": "^3.13.0",
    "lucide-react": "^0.525.0",
    "ogl": "^1.0.11",
    "postprocessing": "^6.37.7",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-helmet-async": "^2.0.5",
    "react-router-dom": "^7.7.0",
